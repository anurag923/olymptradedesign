.sentiment {
    // background-color: var(--bg-alpha);
    bottom: 0;
    display: flex;
    flex-direction: column;
    justify-content: space-between;
    left: 0;
    padding: .3125rem 0;
    position: absolute;
    top: 0;
    width: 1.6rem;
    z-index: 1;
}

.sentiment--text {
    display: block !important;
    color: #fff;
    font-size: .625rem;
    font-weight: 700;
    line-height: .875rem;
    text-align: center;
    padding-bottom: .625rem;
}
.sentiment--rects {
    display: flex;
    flex-direction: column;
    height: calc(100% - 5.625rem);
    justify-content: space-between;
    margin: auto;
    position: relative;
    width: .25rem;
}
.sentiment--rect__up {
    background: #f15131;
    // height: 50%;
    margin-bottom: .03125rem;
    padding: 3px;
}
@media (min-width: 650px){
.sentiment--rect {
    border-radius: .21875rem;
}
}
.sentiment--rect__down {
    background: #1fbf75;
    flex: 1;
    padding: 3px;
    margin-top: .03125rem;
}
@media (min-width: 650px){
.sentiment--rect {
    border-radius: .21875rem;
}
}
.trade-frame__section:first-child .chart-status {
