}

.chart-status__label {
    background: #1fbf75;
    border-radius: 50%;
    // height: .25rem;
    left: 0;
    position: absolute;
    // top: .3125rem;
    // width: .25rem;
}
.chart-status__title {
    margin-left: .875rem;
}
.chart-status__time {
    margin-left: .3125rem;
}



