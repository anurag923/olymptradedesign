<ion-header>
  <ion-toolbar>
    <ion-title>
        <div class="container pt-2">
        <ion-grid  class=" p-0 md hydrated">
          <ion-row  class="md hydrated ">
            <ion-col   size-lg="4" size-md="4" class="home-tabs md hydrated" style="">
              <ion-title  class="heading  md title-default hydrated">
                <div class="com-header__box ">
                  <a class="com-header__logo home-page-title" target="_self" rel="noopener noreferrer">
                    <img src="../../assets/images/OLYMPUS.jpg" alt="">
                  </a>
                  <a href="" class="dispaly-icon">
                    <ion-icon name="disc-outline"></ion-icon>
                   </a>
                    <ion-menu side="start" color="primary" content-id="main-content" class="dispaly-icon-menu">
                      <ion-header>
                        <ion-toolbar translucent class="mob">
                          <ion-title>Menu</ion-title>
                        </ion-toolbar>
                      </ion-header>
                      <ion-content>
                        <ion-list>
                          <ion-item>
                            <ion-icon name="mail" slot="start"></ion-icon>
                            <ion-label>Inbox</ion-label>
                          </ion-item>
                          <ion-item>
                            <ion-icon name="paper-plane" slot="start"></ion-icon>
                            <ion-label>Outbox</ion-label>
                          </ion-item>
                          <ion-item>
                            <ion-icon name="heart" slot="start"></ion-icon>
                            <ion-label>Favorites</ion-label>
                          </ion-item>
                          <ion-item>
                            <ion-icon name="archive" slot="start"></ion-icon>
                            <ion-label>Archived</ion-label>
                          </ion-item>
                          <ion-item>
                            <ion-icon name="trash" slot="start"></ion-icon>
                            <ion-label>Trash</ion-label>
                          </ion-item>
                          <ion-item>
                            <ion-icon name="warning" slot="start"></ion-icon>
                            <ion-label>Spam</ion-label>
                          </ion-item>
                        </ion-list>
                      </ion-content>
                    </ion-menu> 
                    <div class="ion-page dispaly-icon" id="main-content" >
                      <ion-header>
                        <ion-toolbar>
                          <ion-buttons slot="start">
                            <ion-menu-button></ion-menu-button>
                          </ion-buttons>
                          <ion-title>Inbox</ion-title>
                        </ion-toolbar>
                      </ion-header>
                      <ion-content class="ion-padding">
                        <ion-button  onclick="openMenu()">Open Menu</ion-button>
                      </ion-content>
                    </div>
                </div>

              </ion-title>
            </ion-col>
            <ion-col  size-lg="4" size-md="4"  class="home-tabs md hydrated" >  
               <nav class="com-header-links home-page-title">
                  <ul class="com-header-links__list">
                    <!-- <li class="com-header-links__item">
                      <a class="com-header-c-nav-link" href="#">
                        <span >About Us</span>
                      </a>
                    </li> -->
                    <li class="com-header-links__item">
                      <a class="com-header-c-nav-link" href="#">
                        <span >Help Center</span>
                      </a>
                    </li>
                    <li class="com-header-links__item">
                      <a class="com-header-c-nav-link" href="">
                        <span >Assets</span>
                      </a>
                    </li>
                    <li class="com-header-links__item">
                      <a class="com-header-c-nav-link" href="#">
                        <span>Insights</span>
                      </a>
                    </li>
                  </ul>
                </nav>
          
            </ion-col>
            <ion-col  size-md="4" size-lg="4" class="home-tabs md hydrated btn-right" >
              <div>
                <!-- <ion-button  color="light" class="omp_login" slot="end" (click)="loginpage()">Login</ion-button> -->
               <ion-button   color="primary" class="omp_login" slot="end">Start Trading</ion-button>
              </div>
            </ion-col>
          </ion-row>     
        </ion-grid>
        </div>
    </ion-title>
  </ion-toolbar>
</ion-header>

