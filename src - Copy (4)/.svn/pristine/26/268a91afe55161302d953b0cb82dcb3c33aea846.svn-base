.help-sec2-title {
    position: relative;
    margin-bottom: 1.5rem;
    font-size: 1.312rem;
    line-height: 1.75rem;
}
.help-sec2-inner, ._3t3pNEH {
    transition: .2s ease-out;
}

.help-sec2-inner {
    display: flex;
    position: relative;
    flex-direction: column;
    /* min-height: 8.5rem; */
    padding: 1rem;
    overflow: hidden;
    min-height: 10.5rem;
    border-radius: 0.75rem;
    background-color:#203146;
    box-shadow: 0 0.375rem 0.75rem rgba(18, 27, 38, 0.5);
    color: #0094ff;
    cursor: pointer;
}
.help-sec2-span {
    flex-grow: 2;
}
.help-sec2-inner-text {
    display: block;
    color: #fff;
    font-size: 0.8rem;
    font-weight: 600;
    line-height: 1rem;
    --body-fz: 0.875rem;
    
}

.help-sec2-inner:hover {
    background-color: #0094ff;
    color: #fff;
    text-decoration: none;
}
.flogicon img{
    width: 8%;
    margin-right: 10px;
}
.help-marg{
    margin-bottom: 30px;
}
.help-flex{
    display: flex !important;
}