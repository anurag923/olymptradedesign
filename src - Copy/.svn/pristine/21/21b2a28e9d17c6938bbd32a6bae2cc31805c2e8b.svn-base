.Checkbox-module{
  -webkit-tap-highlight-color: transparent;
  align-items: flex-start;
  cursor: pointer;
  display: inline-flex;
  flex-wrap: nowrap;
  
}
.Checkbox-module-item {
  height: 1.5rem;
  left: 0;
  margin: 0;
  opacity: 0;
  position: absolute;
  width: 1.5rem;
  z-index: -1;
}
.Checkbox-module-item_inner {
  align-items: center;
  background-color: #1c2a3b;
  background-position: 50%;
  background-repeat: no-repeat;
  background-size: 50% 50%;
  border: .0625rem solid #2a405b;
  border-radius: calc(0.5rem/2);
  box-sizing: border-box;
  display: inline-flex;
  flex-shrink: 0;
  height: 1.5rem;
  justify-content: center;
  pointer-events: none;
  position: relative;
  text-align: center;
  transition: background-color .2s ease-out;
  width: 1.5rem;
}
.Checkbox-module-item_inner:after {
  border-bottom: .125rem solid;
  border-left: .125rem solid;
  border-radius: var(--corner-radius-gamma);
  color: var(--accent-alpha);
  content: "";
  display: block;
  height: .3125rem;
  opacity: 0;
  position: absolute;
  top: .4375rem;
  transform: scale(0) rotate(
-45deg);
  transition: opacity .2s ease-out,transform .2s ease-out;
  width: .625rem;
}

