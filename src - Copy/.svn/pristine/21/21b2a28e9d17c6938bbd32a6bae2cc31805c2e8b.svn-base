.icon-inner {
  align-items: center;
  border-radius: 50%;
  cursor: pointer;
  display: inline-flex;
  font-size: 1.5rem;
  height: 3.5rem;
  justify-content: center;
  line-height: 1;
  position: relative;
  text-align: center;
  text-decoration: none;
  -webkit-user-select: none;
  -moz-user-select: none;
  user-select: none;
  width: 3.5rem;
}
.icon-inner:after {
  background-color: #000;
  border-radius: 50%;
  content: "";
  height: 100%;
  left: 0;
  opacity: 0;
  position: absolute;
  top: 0;
  transition: opacity .25s ease-in-out;
  width: 100%;
}













